{
  "extends": "astro/tsconfigs/strict",
  "exclude": ["dist/**", "node_modules/**", "public/games/**", ".eslint.cjs"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@icons/*": ["src/icons/*"],
      "@i18n/*": ["src/i18n/*"],
      "@json/*": ["src/json/*"]
    }
  }
}
